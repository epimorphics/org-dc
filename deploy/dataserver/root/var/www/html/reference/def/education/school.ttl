:PostalAddress a owl:class;
    rdfs:label 'address';
    rdfs:comment 'A postal address.'.
    
:address a owl:ObjectProperty;
    rdfs:label 'street';
    rdfs:range :PostalAddress;
    meta:triggerColumn 'Street';
    meta:translatedProp :vcardAddr;
    meta:translationOp config:vcardAddrTransform.
        
:address1 a owl:DatatypeProperty;
    rdfs:label 'address line 1';
    rdfs:comment 'Line 1 of a postal address';
    rdfs:domain :Address;
    rdfs:range xsd:string;
    meta:columnName 'Street';
    meta:pathProp :address;
    meta:columnCategory 'SIMPLE_STRING';
    meta:columnCategory 'STRUCT_PART'.
    
:address2 a owl:DatatypeProperty;
    rdfs:label 'address line 2';
    rdfs:comment 'Line 2 of a postal address';
    rdfs:domain :Address;
    rdfs:range xsd:string;
    meta:columnName 'Locality';
    meta:pathProp :address;
    meta:columnCategory 'SIMPLE_STRING';
    meta:columnCategory 'STRUCT_PART'.
    
:address3 a owl:DatatypeProperty;
    rdfs:label 'address line 3';
    rdfs:comment 'Line 3 of a postal address';
    rdfs:domain :PostalAddress;
    rdfs:range xsd:string;
    meta:columnName 'Address3';
    meta:pathProp :address;
    meta:columnCategory 'SIMPLE_STRING';
    meta:columnCategory 'STRUCT_PART'.
    
:town a owl:DatatypeProperty;
    rdfs:label 'town';
    rdfs:comment 'Town or city part of a postal address';
    rdfs:domain :PostalAddress;
    rdfs:range xsd:string;
    meta:columnName 'Town';
    meta:pathProp :address;
    meta:columnCategory 'SIMPLE_STRING';
    meta:columnCategory 'STRUCT_PART'.
    
:region a owl:DatatypeProperty;
    rdfs:label 'region';
    rdfs:comment 'The region part of a postal address.';
    rdfs:domain :PostalAddress;
    rdfs:range xsd:string;
    meta:columnName 'County (name)';
    meta:pathProp :address;
    meta:columnCategory 'SIMPLE_STRING';
    meta:columnCategory 'STRUCT_PART'.
    
:postcode a owl:DatatypeProperty;
    rdfs:label 'postcode';
    rdfs:comment 'The post code for a postal address.';
    rdfs:domain :PostalAddress;
    rdfs:range xsd:string;
    meta:columnName 'Postcode';
    meta:pathProp :address;
    meta:columnCategory 'SIMPLE_STRING';
    meta:columnCategory 'STRUCT_PART'.    
    
:vcard a owl:ObjectProperty;
    rdfs:label 'vcard';
    rdfs:comment 'The vcard for an entity';
    rdfs:range vcard:VCard.
    
