<rdf:RDF
    xmlns:sdmx-subject="http://purl.org/linked-data/sdmx/2009/subject#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:sdmx-dimension="http://purl.org/linked-data/sdmx/2009/dimension#"
    xmlns:sdmx-measure="http://purl.org/linked-data/sdmx/2009/measure#"
    xmlns:sdmx-attribute="http://purl.org/linked-data/sdmx/2009/attribute#"
    xmlns:coins-data="http://source.data.gov.uk/data/coins/"
    xmlns:coins-dsd="http://finance.data.gov.uk/dsd/coins/"
    xmlns:coins-dimension="http://finance.data.gov.uk/dsd/coins/dimension/"
    xmlns:void="http://rdfs.org/ns/void#"
    xmlns:coins-measure="http://finance.data.gov.uk/dsd/coins/measure/"
    xmlns:j.0="http://www.epimorphics.com/vocabs/coins-generate#"
    xmlns:coins-attribute="http://finance.data.gov.uk/dsd/coins/attribute/"
    xmlns:sdmx-concept="http://purl.org/linked-data/sdmx/2009/concept#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:coins-slicekey="http://finance.data.gov.uk/dsd/coins/slice/"
    xmlns:sdmx="http://purl.org/linked-data/sdmx#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:sdmx-code="http://purl.org/linked-data/sdmx/2009/code#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:j.1="http://finance.data.gov.uk/def/coins/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:qb="http://purl.org/linked-data/cube#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <rdfs:Class rdf:about="http://finance.data.gov.uk/def/coins/Sector">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">sector values class</rdfs:label>
    <skos:prefLabel xml:lang="en">sector values class</skos:prefLabel>
    <rdfs:comment>The class of values in the sector code-list scheme</rdfs:comment>
  </rdfs:Class>
  <sdmx:Concept rdf:about="http://finance.data.gov.uk/def/coins/statistical-concept/sector">
    <rdfs:label xml:lang="en">sector</rdfs:label>
    <skos:prefLabel xml:lang="en">sector</skos:prefLabel>
    <rdfs:comment>The spending sector column denotes whether transactions relate to Central Government (CG) or Local Authority (LA) expenditure. Central Government includes CG bodies’ transactions as well as support to Public Corporations but excludes support for local authorities. Local authority includes LA’s own transactions or CG support for LA</rdfs:comment>
    <dcterms:source rdf:resource="http://www.hm-treasury.gov.uk/d/coins_guidance.pdf"/>
    <j.0:columnName>sector</j.0:columnName>
  </sdmx:Concept>
  <skos:ConceptScheme rdf:about="http://finance.data.gov.uk/def/coins/sector">
    <rdfs:label xml:lang="en">sector code list concept scheme</rdfs:label>
    <skos:prefLabel xml:lang="en">sector code list concept scheme</skos:prefLabel>
    <skos:hasTopConcept>
      <j.1:Sector rdf:about="http://finance.data.gov.uk/def/coins/sector/cg">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:label xml:lang="en">CG</rdfs:label>
        <skos:prefLabel xml:lang="en">CG</skos:prefLabel>
        <skos:notation>cg</skos:notation>
        <rdfs:comment>Central government</rdfs:comment>
        <skos:topConceptOf rdf:resource="http://finance.data.gov.uk/def/coins/sector"/>
      </j.1:Sector>
    </skos:hasTopConcept>
    <skos:hasTopConcept>
      <j.1:Sector rdf:about="http://finance.data.gov.uk/def/coins/sector/la">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:label xml:lang="en">LA</rdfs:label>
        <skos:prefLabel xml:lang="en">LA</skos:prefLabel>
        <skos:notation>la</skos:notation>
        <rdfs:comment>Local authority, including central government support for local authority</rdfs:comment>
        <skos:topConceptOf rdf:resource="http://finance.data.gov.uk/def/coins/sector"/>
      </j.1:Sector>
    </skos:hasTopConcept>
    <skos:hasTopConcept>
      <j.1:Sector rdf:about="http://finance.data.gov.uk/def/coins/sector/s6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:label xml:lang="en">S6</rdfs:label>
        <skos:prefLabel xml:lang="en">S6</skos:prefLabel>
        <skos:notation>s6</skos:notation>
        <rdfs:comment></rdfs:comment>
        <skos:topConceptOf rdf:resource="http://finance.data.gov.uk/def/coins/sector"/>
      </j.1:Sector>
    </skos:hasTopConcept>
  </skos:ConceptScheme>
</rdf:RDF>
