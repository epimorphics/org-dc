  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Corporation">
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Corporation"/>
    <linkedConcept rdf:resource="http://purl.org/vocab/frbr/core#CorporateBody"/>
    <linkedConcept rdf:resource="http://purl.org/goodrelations/v1#BusinessEntity"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Organization"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Corporation</rdfs:label>
    <rdfs:comment>An Organization that has a special legal status &#xD;
that allows a group of persons to act as a CommercialAgent and that insulates &#xD;
the owners (shareholders) from many liabilities that might result from the &#xD;
corporation's operation.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://statistics.data.gov.uk/def/Foundation/intervalDuring">
    <linkedConcept rdf:resource="http://www.w3.org/2006/time#intervalDuring"/>
    <rdfs:range rdf:resource="http://statistics.data.gov.uk/def/Foundation/TimeInterval"/>
