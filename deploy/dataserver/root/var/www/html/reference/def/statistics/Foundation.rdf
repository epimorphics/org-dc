    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Region"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Temporal region</rdfs:label>
    <rdfs:comment>A region of time such as a specific instant or time interval. Can also be used for things like Geological ages.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Division">
