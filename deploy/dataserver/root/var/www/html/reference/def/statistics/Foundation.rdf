<rdf:RDF
    xmlns:protonTop="http://proton.semanticweb.org/2005/04/protont#"
    xmlns:sumo="http://www.ontologyportal.org/translations/SUMO.owl.txt#"
    xmlns:protonUpper="http://proton.semanticweb.org/2005/04/protonu#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:geonames="http://www.geonames.org/ontology#"
    xmlns:swbpTime="http://www.w3.org/2006/time#"
    xmlns="http://statistics.data.gov.uk/def/Foundation/"
    xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
    xmlns:skos="http://www.w3.org/2008/05/skos#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:frbr="http://purl.org/vocab/frbr/core#"
    xmlns:goodRelations="http://purl.org/goodrelations/v1#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos-xl="http://www.w3.org/2008/05/skos-xl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:dolce="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="http://statistics.data.gov.uk/def/Foundation/">
    <owl:imports rdf:resource="http://www.w3.org/2008/05/skos-xl"/>
    <owl:imports rdf:resource="http://www.w3.org/2008/05/skos"/>
    <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <rdfs:comment>An example foundation ontology developed to accompany the experimental EDUBase dataset. Based on Dolce-lite. Utilizes SKOS, SKOS-XL and Dublin Core Elements. Cross links to FRBR ontology, FOAF, DOLCE-lite and Proton.</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/InformationResource">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/date"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/creator"/>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/date"/>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/title"/>
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/ProductInstance"/>
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Agent"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Object"/>
    </rdfs:subClassOf>
    <linkedConcept rdf:resource="http://proton.semanticweb.org/2005/04/protont#InformationResource"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Information resource</rdfs:label>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/source"/>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/creator"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Agent"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/rights"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
        <owl:allValuesFrom>
          <rdf:Description rdf:about="http://www.w3.org/2008/05/skos#Concept">
            <rdfs:subClassOf>
              <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Abstract"/>
            </rdfs:subClassOf>
          </rdf:Description>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/contributor"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Agent"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/type"/>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/format"/>
    <rdfs:comment>InformationResource denotes an information resource with identity, as defined in Dublin Core (DC2003ISO). A document (physical or electronic), message or other information object. We recommend use of Dublic Core elements for basic descriptive metadata and guide usage by restricting the range of some dc elements.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/GeoLocation"/>
    </owl:disjointWith>
    <recommendedProperty rdf:resource="http://purl.org/dc/elements/1.1/contributor"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/source"/>
        <owl:allValuesFrom rdf:resource="http://statistics.data.gov.uk/def/Foundation/InformationResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/TimeInterval">
    <linkedConcept rdf:resource="http://www.w3.org/2006/time#Interval"/>
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#time-interval"/>
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#TimeInterval"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/TemporalRegion"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Time interval</rdfs:label>
    <rdfs:comment>An interval of time.  Note that a TimeInterval has both an extent and a location on a timeline.  Note too that a TimeInterval has no gaps, i.e. this class contains only convex time intervals.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Agent">
    <linkedConcept rdf:resource="http://proton.semanticweb.org/2005/04/protont#Agent"/>
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/GeoLocation"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/ProductInstance"/>
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/InformationResource"/>
    <linkedConcept rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Agent"/>
    <rdfs:label>Agent</rdfs:label>
    <rdfs:comment>Something or someone that can act on its own and produce changes in the world.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Object"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Meeting">
    <linkedConcept rdf:resource="http://www.owl-ontologies.com/file1.owl#Meeting"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://statistics.data.gov.uk/def/Foundation/participant"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://statistics.data.gov.uk/def/Foundation/Agent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Event"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Meeting</rdfs:label>
    <rdfs:comment>Any sort of formal or semi-formal meeting such as a business meeting.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Entity">
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#particular"/>
    <rdfs:label>Entity</rdfs:label>
    <rdfs:comment>Root of the foundational ontology. Includes objects, processes and abstractions.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Man">
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Man"/>
    <linkedConcept rdf:resource="http://www.owl-ontologies.com/file1.owl#Man"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Person"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Man</rdfs:label>
    <rdfs:comment>Class of male persons.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Person">
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Group"/>
    </owl:disjointWith>
    <linkedConcept rdf:resource="http://proton.semanticweb.org/2005/04/protont#Person"/>
    <linkedConcept rdf:resource="http://purl.org/vocab/frbr/core#Person"/>
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Human"/>
    <linkedConcept rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Agent"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Person</rdfs:label>
    <rdfs:comment>An individual, a human being</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/ReligousOrganization">
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#ReligiousOrganization"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Organization"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Religous group</rdfs:label>
    <rdfs:comment>An Organization whose members share a set of religious beliefs.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/WeightedRelation">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Property rdf:about="http://statistics.data.gov.uk/def/Foundation/weight"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/NaryRelation"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Weighted relation</rdfs:label>
    <rdfs:comment>A relation containing a weight (e.g. an associated probablilty) and an object or literal value.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Endurant">
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Perdurant"/>
    </owl:disjointWith>
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#endurant"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Entity"/>
    <rdfs:label>Endurant</rdfs:label>
    <rdfs:comment>Endurants exists in time, values of some properties might different over time but the Endurant has a constant identity. Endurants can participate in processes. Philosophically speaking they are independent essential wholes.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Organization">
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Organization"/>
    <linkedConcept rdf:resource="http://proton.semanticweb.org/2005/04/protont#Organization"/>
    <linkedConcept rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Group"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Organization</rdfs:label>
    <rdfs:comment>An Organization is a corporate or similar institution. The members of an Organization typically have a common purpose or function. Note that this class also covers divisions, departments, etc. of organizations.  For example, both the Shell Corporation and the accounting department at Shell would both be instances of Organization. </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Abstract">
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Entity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Abstract</rdfs:label>
    <rdfs:comment>Something, which neither happens nor exists. &#xD;
The main characteristic of abstract entities is that  they do not have spatial &#xD;
nor temporal qualities, and they are not qualities themselves.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/ObjectFeature">
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Object"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/AmountOfMatter"/>
    </owl:disjointWith>
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#feature"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://statistics.data.gov.uk/def/Foundation/partOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Object"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Endurant"/>
    <rdfs:label xml:lang="en">Object feature</rdfs:label>
    <rdfs:comment>Something which can't exist on its own but is a feature of some physical object, e.g. a bump, hole, region, spot.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/TimeDependentRelation">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Property rdf:about="http://statistics.data.gov.uk/def/Foundation/inContext"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://statistics.data.gov.uk/def/Foundation/TimeInterval"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/QualifiedBinaryRelation"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Time dependent relation</rdfs:label>
    <rdfs:comment>Reification of a time-dependent relation. A special case of a QualifiedBinaryRelation where the context should be a time interval.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/BusinessActivity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Process"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Business activity</rdfs:label>
    <rdfs:comment>Some shared business endeavour. Use for ongoing activities as opposed to projects that have clear cut start/end time courses.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Object">
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/ObjectFeature"/>
    <owl:disjointWith>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/AmountOfMatter"/>
    </owl:disjointWith>
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#physical-object"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://statistics.data.gov.uk/def/Foundation/partOf"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://statistics.data.gov.uk/def/Foundation/Object"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Endurant"/>
    <rdfs:label>Object</rdfs:label>
    <rdfs:comment>Physical objects. Endurants with unity and some independent existence.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/AmountOfMatter">
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/ObjectFeature"/>
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/Object"/>
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#amount-of-matter"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://statistics.data.gov.uk/def/Foundation/partOf"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://statistics.data.gov.uk/def/Foundation/AmountOfMatter"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Endurant"/>
    <rdfs:label>Amount of matter</rdfs:label>
    <rdfs:comment>Things like "water", "clay" which have no unity (the combination of two amounts of clay is just a single amount of clay), aka "stuff".</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Relation">
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Relation"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Abstract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Relation</rdfs:label>
    <rdfs:comment>Class of relations. Mostly used for when we are reifiying complex relations as objects and so does not itself break the DL constraints, could be used as a category of general relations as done in SUMO in which case we would be forced into OWL/full.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Instant">
    <linkedConcept rdf:resource="http://www.w3.org/2006/time#Instant"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/TemporalRegion"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Instant</rdfs:label>
    <rdfs:comment>A point in time. Can have either a hasBeginning or hasEnding value, if it has both they should be the same.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Region">
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#region"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Abstract"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Region</rdfs:label>
    <rdfs:comment>A region of some measurement space such as a time interval, spatial region or colour space.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/GeoLocation">
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/InformationResource"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Geographic Location</rdfs:label>
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/Agent"/>
    <linkedConcept rdf:resource="http://proton.semanticweb.org/2005/04/protont#Location"/>
    <rdfs:comment>A geospatial location, encompasses locations with extent (e.g. countries) as well as features which can be treated as point-like for some purposes (e.g. a bridge or road junction). Normally designated by a Geonames URI.</rdfs:comment>
    <linkedConcept rdf:resource="http://purl.org/vocab/frbr/core#place"/>
    <owl:disjointWith rdf:resource="http://statistics.data.gov.uk/def/Foundation/ProductInstance"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Object"/>
    <linkedConcept rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#GeographicArea"/>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/GeoFeature">
    <linkedConcept rdf:resource="http://www.geonames.org/ontology#Feature"/>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/GeoLocation"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Geo Feature</rdfs:label>
    <rdfs:comment>A geographical feature such as a river, mountain, including man made features such as cities and bridges. Ususally designated by a Geonames URI with associated descriptive information.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/SpatialRegion">
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#spatial-region"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://statistics.data.gov.uk/def/Foundation/partOf"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://statistics.data.gov.uk/def/Foundation/SpatialRegion"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://statistics.data.gov.uk/def/Foundation/Region"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Spatial region</rdfs:label>
    <rdfs:comment>A region of space in some measurement system including a polygonal boundary of a geographic space or a region of 3D space.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/FnOfTime_int">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/Datum_int"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://statistics.data.gov.uk/def/Foundation/datum"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/FnOfTime"/>
    </rdfs:subClassOf>
    <rdfs:comment>Represents an integer-valued function of time</rdfs:comment>
    <rdfs:label>Function of time (integer)</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://statistics.data.gov.uk/def/Foundation/TemporalRegion">
    <linkedConcept rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#temporal-region"/>
    <linkedConcept rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
